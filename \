package main

import (
	"fmt"
	"strings"

	u "github.com/hvieira512/aoc2024/cmd/utils"
)

func partOne(lines []string) int {
	result := 0

	for _, line := range lines {
		aux := strings.Split(line, "mul(")
		fmt.Println(aux)
	}

	return result
}

func main() {
	u.RenderDayHeader(3)

	lines, _ := u.ReadLines("cmd/day3/example.txt")

	fmt.Printf("Part 1: %v\n", partOne(lines))
}
